{
  "name": "PDF",
  "type": "system/SIMOS/Blueprint",
  "extends": ["system/SIMOS/DefaultUiRecipes","system/SIMOS/NamedEntity"],
  "description": "Contains metadata for a PDF, and a id for the PDF Blob",
  "attributes": [
    {
      "type": "system/SIMOS/BlueprintAttribute",
      "name": "blob_reference",
      "description": "id for the data blob",
      "attributeType": "string",
      "contained": true
    }, {
      "type": "system/SIMOS/BlueprintAttribute",
      "name": "size",
      "description": "Size of the PDF in bytes",
      "attributeType": "integer",
      "default": "0",
      "optional": true,
      "contained": true
    }, {
      "type": "system/SIMOS/BlueprintAttribute",
      "name": "tags",
      "description": "List of tags used for indexing",
      "attributeType": "string",
      "dimensions": "*",
      "optional": true,
      "contained": true
    }, {
      "type": "system/SIMOS/BlueprintAttribute",
      "name": "author",
      "description": "Author of the PDF",
      "attributeType": "string",
      "default": "",
      "contained": true,
      "optional": true
    }
  ],
  "storageRecipes": [],
  "uiRecipes": [
    {
      "type": "system/SIMOS/UiRecipe",
      "name": "PDFView",
      "description": "Embedded PDF View",
      "plugin": "default-pdf",
      "attributes": []
    }
  ]
}
